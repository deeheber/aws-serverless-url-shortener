{
  "name": "aws-serverless-url-shortener",
  "version": "0.1.0",
  "bin": {
    "aws-serverless-url-shortener": "bin/aws-serverless-url-shortener.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
